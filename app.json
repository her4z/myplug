{
  "expo": {
    "name": "myplug",
    "slug": "myplug",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.her4z.myplug"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Sneakers's Buy/Sell application.",
    "githubUrl": "https://github.com/her4z/myplug.git"
  }
}
